{
  "name": "Emoji Magic",
  "short_name": "Emoji Magic - Find Emojis Fast and Easy",
  "description": "Find Emoji Easily. Safe and secure: only asks for clipboard permissions.",
  "version": "1.0.9",
  "homepage_url": "https://www.simple.gy/projects/emoji-magic/",
  "offline_enabled": true,
  "manifest_version": 2,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Emoji Magic",
    "default_popup": "src/browser_action.html"
  },
  "permissions": [
    "clipboardRead",
    "clipboardWrite"
  ],
  "omnibox": {
    "keyword": "emoji search find keyword emojis smileys unicode bitmoji"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+O",
        "mac": "Command+Shift+O",
        "chromeos": "Ctrl+Shift+O",
        "linux": "Ctrl+Shift+O"
      }
    }
  }
}